{
  "manifest_version": 2,

  "name": "doubanFM-songList",
  "description": "Get your song list of your favourite on doubanFM",
  "version": "0.0.1",
  "omnibox": { "keyword" : "sina" },

  "browser_action": {},
  


  "content_scripts": [ {
      "js": [ "popup.js" ],
      "matches": [ "http://t.sina.com/*", "http://weibo.com/*", "http://*.baidu.com/*" ],
      "run_at": "document_end"
   } ]
}
